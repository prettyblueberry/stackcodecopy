{
  "name": "StackCodeCopy",
  "description": "Adds a button for easily copying formatted code snippets from stackoverflow",
  "version": "0.0.1",
  "manifest_version": 4,
  "content_scripts": [
    {
      "matches": ["http://stackoverflow.com/*", "https://stackoverflow.com/*"],
      "js": ["copy.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "768": "stack_logo.png"
  }
}